import scrapy


class AltyaziItem(scrapy.Item):
    url = scrapy.Field()
    name = scrapy.Field()
    lang = scrapy.Field() 
    commenters = scrapy.Field() 
    # download = scrapy.Field()
    #id = scrapy.Field()  
    
